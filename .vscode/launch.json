{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Webhook Route",
        "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
        "args": ["dev"],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "runtimeArgs": ["--inspect"],
        "port": 9229,
        "envFile": "${workspaceFolder}/.env.local",
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }